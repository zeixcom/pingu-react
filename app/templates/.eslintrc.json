{
  "extends": [
    "airbnb"
  ],
  "rules": {
    "react/jsx-curly-spacing": [2, "always"],
    "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }]
  },
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
